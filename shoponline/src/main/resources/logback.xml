<?xml version="1.0" encoding="UTF-8"?>
<configuration>

<appender name="consoleAppender" class="ch.qos.logback.core.ConsoleAppender">
	<encoder>
<pattern>%-10level %msg %d{HH:mm:ss.SSS} %logger{36} %n</pattern>
	
	</encoder>

</appender>
<logger name="com.shop" level="DEBUG" additivity="false">
<appender-ref ref="consoleAppender"></appender-ref>
</logger>
<root level="INFO">
<appender-ref ref="consoleAppender"></appender-ref>

</root>
</configuration>